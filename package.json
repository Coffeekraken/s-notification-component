{
  "name": "coffeekraken-s-notification-component",
  "version": "1.0.0",
  "description": "Display nice and fully customizable toast like notification.",
  "homepage": "http://coffeekraken.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/coffeekraken/s-notification-component"
  },
  "main": "dist/index.js",
  "watch": {
    "dist.js": "src/**/*.js",
    "doc.js": "src/**/*.js",
    "doc.css": "src/**/*.scss",
    "demo": "code-playground.config.js"
  },
  "scripts": {
    "dist": "npm run dist.js",
    "dist.js": "babel src -d dist",
    "demo": "coffeekraken-code-playground",
    "doc": "rm -rf doc && npm run doc.js && npm run doc.css",
    "doc.js": "coffeekraken-docblock-to-markdown -f 'src/**/*.js' -d doc --remove-path src/",
    "doc.css": "coffeekraken-docblock-to-markdown -f 'src/**/*.scss' -d doc --remove-path src/",
    "test": "coffeekraken-testing-stack",
    "start": "npm-watch",
    "watch": "npm-watch"
  },
  "keywords": [
    "notification",
    "webcomponent",
    "component",
    "html",
    "sugar",
    "read",
    "more",
    "coffee",
    "kraken",
    "coffeekraken"
  ],
  "author": "Coffeekraken <hello@coffeekraken.io>",
  "contributors": [
    {
      "name": "Olivier Bossel",
      "email": "olivier.bossel@gmail.com",
      "url": "http://olivierbossel.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "coffeekraken-sugar": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "coffeekraken-code-playground": "0.0.2",
    "coffeekraken-docblock-to-markdown": "0.0.2",
    "coffeekraken-testing-stack": "0.0.2",
    "concurrently": "^3.1.0",
    "http-server": "^0.9.0",
    "node-sass": "^4.0.0",
    "nodemon": "^1.11.0",
    "npm-watch": "^0.1.8",
    "webcomponents.js": "^0.7.24",
    "webpack": "^2.0.0"
  }
}
